  "keywords": [
    "vattenfall",
    "api",
    "client",
    "vattenfall-europe-sales"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "package.json",
    "README.md",
    "LICENSE"
